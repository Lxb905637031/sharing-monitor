{
    "name": "@sharing/monitor-sdk-core",
    "version": "1.0.0",
    "main": "build/cjs/index.js",
    "module": "build/esm/index.mjs",
    "types": "build/types/index.d.ts",
    "exports": {
        "./package.json": "./package/json",
        ".": {
            "import": {
                "types:": "./build/types/index.d.ts",
                "default": "./build/esm/index.mjs"
            },
            "require": {
                "types": "./build/types/index.d.ts",
                "default": "./build/cjs/index.js"
            }
        }
    },
    "scripts": {
        "build:transpile": "tsup",
        "build:types": "tsc -p tsconfig.types.json",
        "build": "pnpm build:transpile && pnpm build:types",
        "build:watch:types": "tsc -p tsconfig.types.json",
        "build:watch:transpile": "tsup --watch",
        "build:watch": "pnpm build:watch:types & pnpm build:watch:transpile",
        "clean": "rimraf build",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": ""
}
